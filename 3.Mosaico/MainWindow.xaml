<Window x:Class="_3.Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_3.Mosaico"
        mc:Ignorable="d"
        Title="Mosaico"
        
        Height="450" Width="800"
        MinHeight="470" MinWidth="450" 
        Icon="32.ico">
    <DockPanel>
        <TextBlock DockPanel.Dock="Left" Background="BlueViolet" Width="100"></TextBlock>
        <TextBlock DockPanel.Dock="Top" Background="BlueViolet" Height="100"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Background="ForestGreen" Width="100"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Background="ForestGreen" Height="100"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Background="OrangeRed" Width="100"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Background="OrangeRed" Height="100"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Background="DeepPink" Width="100"></TextBlock>
        <TextBlock DockPanel.Dock="Top" Background="DeepPink" Height="100"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Background="Black"></TextBlock>

    </DockPanel>
</Window>
